{
  "editor.formatOnSave": false,
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.onIgnoredFiles": "off",
  "eslint.lintTask.options": "--ext .ts,.tsx --ignore-path .eslintignore",
  "eslint.run": "onSave",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "cSpell.spellCheckDelayMs": 200,
  "cSpell.words": [
    "movo"
  ]
}
